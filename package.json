{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "build": "vite build",
    "predev": "node scripts/prebuild.mjs",
    "dev": "vite",
    "preview": "vite preview --port 5173 --strictPort",
    "analyze": "vite build --mode production",
    "bundle:report": "vite build && npx vite-bundle-visualizer",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:e2e:headed": "playwright test -c playwright.config.ts --headed",
    "test:e2e:preview:posix": "PW_BUILD=1 playwright test -c playwright.config.ts",
    "test:e2e:preview:win": "set PW_BUILD=1&& playwright test -c playwright.config.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lcp:trace": "npm run lcp:trace:posix",
    "lcp:trace:posix": "PW_BUILD=1 playwright test -c playwright.config.ts tests/lcp-trace.spec.ts",
    "lcp:trace:win": "set PW_BUILD=1&& playwright test -c playwright.config.ts tests/lcp-trace.spec.ts",
    "lint": "eslint . --cache",
    "lint:ci": "ESLINT_TYPED=1 eslint . --cache",
    "test": "vitest run",
    "test:ui": "vitest",
    "ci": "npm run typecheck && npm run lint && npm run build && npm run test",
    "assets:ktx2": "node --experimental-json-modules scripts/convert-images.mjs",
    "assets:dups": "node --experimental-json-modules scripts/scan-duplicates.mjs",
    "models:pack": "node --experimental-json-modules scripts/pack-gltf.mjs",
    "models:finalize": "node --experimental-json-modules scripts/finalize-models.mjs",
    "models:tile": "node --experimental-json-modules scripts/tile-city.mjs",
    "models:clean": "node --experimental-json-modules scripts/cleanup-model-trash.mjs",
    "compress:glb": "node compress-glb.js",
    "ktx2:install": "echo \"Instala KTX-Software (toktx) desde https://github.com/KhronosGroup/KTX-Software/releases\""
  },
  "dependencies": {
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@react-three/rapier": "^2.1.0",
    "framer-motion": "^12.23.12",
    "howler": "^2.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.0",
    "three": "^0.179.1",
    "three-mesh-bvh": "^0.9.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/extensions": "^4.2.1",
    "@gltf-transform/functions": "^4.2.1",
    "@playwright/test": "^1.56.0",
    "@types/howler": "^2.2.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "draco3dgltf": "^1.5.7",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-glob": "^3.3.3",
    "gl-matrix": "^3.4.3",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "minimatch": "^10.0.3",
    "p-limit": "^7.1.1",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.4",
    "serve": "^14.2.5",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17",
    "three-stdlib": "^2.36.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.7",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-glsl": "^1.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9"
  },
  "overrides": {
    "esbuild": "^0.25.10",
    "tmp": "0.2.4",
    "vite": {
      "esbuild": "^0.25.10"
    },
    "@dimforge/rapier3d-compat": "0.15.0"
  }
}
